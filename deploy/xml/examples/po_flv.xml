<?xml version="1.0" encoding="utf-8"?>
<page>
	
	<config>
		<!-- name of the page -->
		<name>Testing Page</name>
		<!-- type of page -->
		<type>testing</type>
		<!-- size -->
		<pagesize>750,360</pagesize>
		<!-- left, top, right, bottom -->
		<pageborders>10,10,10,10</pageborders>
	</config>
	
	<!-- 
		On stage object data, text fields, etc
		the target attrib of a <text> tag corresponts to a text field named "value_txt"
	 -->
	<pagecontent>
		<!-- title, body, bodyleft and bodyright are adjusted based on page size and page borders -->
		<text target="title"><![CDATA[FLV Player Box Page Object]]></text>
		<text target="body"><![CDATA[]]></text>
		<text target="bodyleft"><![CDATA[]]></text>
		<text target="bodyright"><![CDATA[Simple FLV Player<br><br><b>May one have one per page!</b>]]></text>
	</pagecontent>

	<!-- Page Objects go here -->
	<sheet>
		<object type="flvplayerbox" id="flv1" guid="">
			<position>10,50</position>
			<borderstyle>show</borderstyle>
			<transitionin effect="fade_in" delay="0" mode="play" />
			<url>assets/sample.flv</url>
		</object>
	</sheet>

</page>