	<name>Testing Page</name>
	<!-- type of page -->
	<type>testing</type>
	<!-- size -->
	<pagesize>750,360</pagesize>
	<!-- left, top, right, bottom -->
	<pageborders>10,10,10,10</pageborders>
	<!-- background image for the page -->
	<pagebgimage></pagebgimage>
</config>

